{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "user.js", "use": "@vercel/node" },
    { "src": "home.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/user", "dest": "user.js" },
    { "src": "/api/food", "dest": "home.js" },
    { "src": "/", "dest": "index.js" }
  ]
}
